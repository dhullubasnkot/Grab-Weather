{
  "manifest_version": 3,
  "name": "Weather Checker",
  "version": "1.0",
  "description": "Get current weather by city name.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/weather48.png"
    }
  },
  "permissions": ["geolocation"],
  "icons": {
    "48": "icons/weather48.png"
  }
}
